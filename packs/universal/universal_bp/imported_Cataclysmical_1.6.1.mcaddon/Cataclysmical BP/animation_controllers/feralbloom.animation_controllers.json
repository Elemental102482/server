{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.feralbloom.attack_controller": {
			"states": {
				"default": {
					"transitions": [
						{
							"normal_attack": "query.is_delayed_attacking==1&&query.skin_id==3"
						},
						{
							"bloom_poison": "query.is_casting==1&&query.skin_id==5"
						},
						{
							"heavy_attack": "query.is_delayed_attacking==1&&query.skin_id==4"
						}
					]
				},
				"heavy_attack": {
					"animations": [
						"heavy_attack"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					],
					"on_entry": [
						"@s is_attacking"
					],
					"on_exit": [
						"@s finish_attack",
						"@s attack_types"
					]
				},
				"bloom_poison": {
					"animations": [
						"bloom_poison"
					],
					"transitions": [
						{
							"default": "!query.is_casting"
						}
					],
					"on_entry": [
						"@s is_attacking"
					],
					"on_exit": [
						"@s finish_attack",
						"@s attack"
					]
				},
				"normal_attack": {
					"animations": [
						"normal_attack"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					],
					"on_entry": [
						"@s finish_attack"
					],
					"on_exit": [
						"@s attack_types"
					]
				}
			}
		}
	}
}