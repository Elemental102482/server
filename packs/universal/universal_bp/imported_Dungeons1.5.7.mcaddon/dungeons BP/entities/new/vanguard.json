{
	"format_version": "1.16.0",
	"minecraft:entity": {
		"description": {
			"identifier": "dungeons:vanguard",
			"runtime_identifier": "minecraft:skeleton",
			"is_spawnable": true,
			"is_summonable": true,
			"is_experimental": false
		},
		"component_groups": {
			"dungeons:instant_despawn": {
				"minecraft:instant_despawn": {}
			},
			"dungeons:nameless_despawn": {
				"minecraft:persistent": {},
				"minecraft:entity_sensor": {
					"sensor_range": 48,
					"relative_range": false,
					"minimum_count": 0,
					"maximum_count": 0,
					"event_filters": {
						"any_of": [
							{
								"test": "is_family",
								"subject": "other",
								"value": "nameless_one"
							}
						]
					},
					"event": "dungeons:instant_despawn"
				}
			},
			"dungeons:standard_despawn": {
				"minecraft:despawn": {
					"despawn_from_distance": {}
				},
				"minecraft:experience_reward": {
					"on_death": "query.last_hit_by_player?12+(query.equipment_count*math.random(1,3)):0"
				}
			},
			"dungeons:become_enchanted": {
				"minecraft:transformation": {
					"into": "dungeons:enchanted_vanguard",
					"preserve_equipment": true
				}
			},
			"dungeons:has_shield": {
				"minecraft:is_saddled": {},
				"minecraft:damage_sensor": {
					"triggers": [
						{
							"on_damage": {
								"filters": {
									"all_of": [
										{
											"test": "is_family",
											"subject": "other",
											"value": "mob"
										}
									]
								},
								"event": "dungeons:break_shield"
							},
							"deals_damage": false
						},
						{
							"on_damage": {
								"filters": {
									"all_of": [
										{
											"test": "is_family",
											"subject": "other",
											"value": "player"
										}
									]
								},
								"event": "dungeons:break_shield"
							},
							"deals_damage": false
						},
						{
							"on_damage": {
								"filters": {
									"all_of": [
										{
											"test": "is_family",
											"subject": "other",
											"value": "artefact"
										}
									]
								},
								"event": "dungeons:break_shield"
							},
							"deals_damage": false
						}
					]
				},
				"minecraft:knockback_resistance": {
					"value": 1
				}
			},
			"dungeons:no_shield": {
				"minecraft:damage_sensor": {
					"triggers": [
						{
							"on_damage": {
								"filters": {
									"all_of": [
										{
											"test": "is_family",
											"subject": "other",
											"value": "nameless_one"
										}
									]
								}
							},
							"deals_damage": false
						}
					]
				},
				"minecraft:knockback_resistance": {
					"value": 0
				}
			}
		},
		"components": {
			"minecraft:is_hidden_when_invisible": {},
			"minecraft:loot": {
				"table": "loot_tables/entities/vanguard.json"
			},
			"minecraft:nameable": {},
			"minecraft:breathable": {
				"total_supply": 15,
				"suffocate_time": 0,
				"breathes_water": true
			},
			"minecraft:health": {
				"value": 26,
				"max": 26
			},
			"minecraft:attack": {
				"damage": 1
			},
			"minecraft:collision_box": {
				"width": 0.6,
				"height": 1.9
			},
			"minecraft:equipment": {
				"table": "loot_tables/entities/vanguard_gear.json",
				"slot_drop_chance": [
					{
						"slot": "slot.weapon.mainhand",
						"drop_chance": 0.01
					}
				]
			},
			"minecraft:movement": {
				"value": 0.2
			},
			"minecraft:navigation.walk": {
				"is_amphibious": true,
				"avoid_sun": false,
				"avoid_water": true
			},
			"minecraft:movement.basic": {},
			"minecraft:jump.static": {},
			"minecraft:can_climb": {},
			"minecraft:despawn": {
				"despawn_from_distance": {}
			},
			"minecraft:behavior.hurt_by_target": {
				"priority": 1
			},
			"minecraft:behavior.avoid_mob_type": {
				"priority": 4,
				"entity_types": [
					{
						"filters": {
							"test": "is_family",
							"subject": "other",
							"value": "wolf"
						},
						"max_dist": 6,
						"walk_speed_multiplier": 1.2,
						"sprint_speed_multiplier": 1.2
					}
				]
			},
			"minecraft:behavior.delayed_attack": {
				"priority": 3,
				"track_target": true,
				"require_complete_path": false,
				"random_stop_interval": 0,
				"reach_multiplier": 6.5,
				"cooldown_time": 0.1,
				"speed_multiplier": 1.15,
				"attack_duration": 0.83,
				"hit_delay_pct": 0.6
			},
			"minecraft:behavior.nearest_attackable_target": {
				"priority": 2,
				"reselect_targets": true,
				"must_see": true,
				"entity_types": [
					{
						"filters": {
							"test": "is_family",
							"subject": "other",
							"value": "player"
						},
						"max_dist": 16
					},
					{
						"filters": {
							"test": "is_family",
							"subject": "other",
							"value": "irongolem"
						},
						"max_dist": 16
					},
					{
						"filters": {
							"all_of": [
								{
									"test": "is_family",
									"subject": "other",
									"value": "baby_turtle"
								},
								{
									"test": "in_water",
									"subject": "other",
									"operator": "!=",
									"value": true
								}
							]
						},
						"max_dist": 16
					}
				]
			},
			"minecraft:behavior.random_stroll": {
				"priority": 6,
				"speed_multiplier": 1
			},
			"minecraft:behavior.look_at_player": {
				"priority": 7,
				"look_distance": 8
			},
			"minecraft:behavior.random_look_around": {
				"priority": 8
			},
			"minecraft:type_family": {
				"family": [
					"vanguard",
					"monster",
					"undead",
					"skeleton",
					"mob"
				]
			},
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": true,
				"is_pushable_by_piston": true
			},
			"minecraft:conditional_bandwidth_optimization": {}
		},
		"events": {
			"minecraft:entity_spawned": {
				"sequence": [
					{
						"randomize": [
							{
								"weight": 1,
								"remove": {},
								"add": {
									"component_groups": [
										"dungeons:become_enchanted"
									]
								}
							},
							{
								"weight": 11
							}
						]
					},
					{
						"add": {
							"component_groups": [
								"dungeons:has_shield",
								"dungeons:standard_despawn"
							]
						}
					}
				]
			},
			"dungeons:nameless_minion": {
				"add": {
					"component_groups": [
						"dungeons:has_shield",
						"dungeons:nameless_despawn"
					]
				}
			},
			"dungeons:break_shield": {
				"add": {
					"component_groups": [
						"dungeons:no_shield"
					]
				},
				"remove": {
					"component_groups": [
						"dungeons:has_shield"
					]
				},
				"queue_command": {
					"command": [
						"playsound item.shield.block @a ~~~ 1.0 1.0",
						"playsound random.break @a ~~~ 0.3 1.0"
					]
				}
			},
			"dungeons:instant_despawn": {
				"add": {
					"component_groups": [
						"dungeons:instant_despawn"
					]
				},
				"queue_command": {
					"command": [
						"particle dungeons:vanguard_despawn"
					]
				}
			}
		}
	}
}