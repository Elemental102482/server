{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "dungeons:vanguard",
			"materials": {
				"default": "entity_alphatest"
			},
			"textures": {
				"default": "textures/entity/skeleton/vanguard",
				"glaive": "textures/entity/skeleton/glaive"
			},
			"geometry": {
				"default": "geometry.vanguard"
			},
			"render_controllers": [
				"controller.render.default"
			],
			"animations": {
				"attack_controller": "controller.animation.redstone_golem.attack",
				"move_controller": "controller.animation.redstone_golem.move",
				"attack.rotations": "animation.vanguard.attack",
				"look_at_target_default": "animation.humanoid.look_at_target.default",
				"look_at_target_gliding": "animation.humanoid.look_at_target.gliding",
				"look_at_target_swimming": "animation.humanoid.look_at_target.swimming",
				"head_controller": "controller.animation.humanoid.look_at_target",
				"move": "animation.vanguard.move",
				"bob": "animation.humanoid.bob",
				"shielded": "animation.vanguard.shielded",
				"glaive": "animation.vanguard.glaive"
			},
			"particle_effects": {
				"shield": "dungeons:vanguard_shield_break"
			},
			"scripts": {
				"pre_animation": [
					"variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;"
				],
				"animate": [
					"head_controller",
					"attack_controller",
					"move_controller",
					"bob",
					"glaive",
					{
						"shielded": "!query.is_saddled"
					}
				]
			},
			"enable_attachables": true,
			"spawn_egg": {
				"texture": "egg_vanguard"
			}
		}
	}
}