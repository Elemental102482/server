{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.mooshroom_monstrosity.attack": {
			"initial_state": "default",
			"states": {
				"attacking": {
					"animations": [
						"smash"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					],
					"blend_transition": 0.2
				},
				"default": {
					"transitions": [
						{
							"attacking": "query.is_delayed_attacking==1"
						}
					],
					"blend_transition": 0.3
				}
			}
		},
		"controller.animation.mooshroom_monstrosity.shoot": {
			"initial_state": "default",
			"states": {
				"shooting": {
					"animations": [
						"shoot"
					],
					"transitions": [
						{
							"default": "q.modified_move_speed>0.1||!q.has_target||q.is_casting||q.variant==0"
						}
					],
					"blend_transition": 0.5
				},
				"default": {
					"transitions": [
						{
							"shooting": "q.modified_move_speed<=0.1&&q.variant&&q.has_target&&!q.is_casting"
						}
					],
					"blend_transition": 0.05
				}
			}
		}
	}
}