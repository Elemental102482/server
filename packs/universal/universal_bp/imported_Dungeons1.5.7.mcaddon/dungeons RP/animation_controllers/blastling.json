{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.blastling.move": {
			"initial_state": "default",
			"states": {
				"moving": {
					"animations": [
						"move",
						"move_arms"
					],
					"transitions": [
						{
							"default": "q.modified_move_speed<=0.1"
						}
					],
					"blend_transition": 0.5
				},
				"default": {
					"transitions": [
						{
							"moving": "q.modified_move_speed>0.1"
						}
					],
					"blend_transition": 0.5
				}
			}
		},
		"controller.animation.blastling.aim": {
			"initial_state": "default",
			"states": {
				"aim": {
					"animations": [
						"aim"
					],
					"transitions": [
						{
							"default": "q.is_saddled"
						}
					],
					"blend_transition": 0.6
				},
				"default": {
					"transitions": [
						{
							"aim": "!q.is_saddled"
						}
					],
					"blend_transition": 0.6
				}
			}
		}
	}
}