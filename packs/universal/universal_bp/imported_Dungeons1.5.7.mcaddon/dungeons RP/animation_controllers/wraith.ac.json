{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.wraith.move": {
			"initial_state": "default",
			"states": {
				"moving": {
					"animations": [
						"move"
					],
					"transitions": [
						{
							"default": "q.modified_move_speed<=0.1||q.variant==1||query.is_casting==1"
						}
					],
					"blend_transition": 0.1
				},
				"default": {
					"transitions": [
						{
							"moving": "q.modified_move_speed>0.1&&q.variant!=1&&query.is_casting<=0"
						}
					],
					"blend_transition": 0.3
				}
			}
		}
	}
}