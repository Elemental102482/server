{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.dungeons_iron_golem": {
			"geometry": "geometry.default",
			"arrays": {
				"textures": {
					"array.skins": [
						"texture.default",
						"texture.glow"
					]
				}
			},
			"textures": [
				"array.skins[query.is_saddled]"
			]
		},
		"controller.render.dungeons_iron_golem_cracks": {
			"arrays": {
				"textures": {
					"Array.cracks": [
						"Texture.cracked_high",
						"Texture.cracked_med",
						"Texture.cracked_low",
						"Texture.cracked_none"
					]
				}
			},
			"geometry": "geometry.default",
			"textures": [
				"array.cracks[math.clamp(query.health/25,0,3)]"
			]
		}
	}
}