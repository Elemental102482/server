				"uv": {
					"texture_width": 1,
					"texture_height": 12,
					"flipbook": {
						"base_UV": [
							0,
							0
						],
						"size_UV": [
							1,
							1
						],
						"step_UV": [
							0,
							1
						],
						"max_frame": 13,
						"stretch_to_lifetime": true
					}
				}
