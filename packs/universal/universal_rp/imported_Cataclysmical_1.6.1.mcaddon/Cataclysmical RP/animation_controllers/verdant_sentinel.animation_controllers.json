{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.verdant_sentinel.move_controller": {
			"states": {
				"unconscious": {
					"animations": [
						"unconscious"
					],
					"transitions": [
						{
							"awaken": "query.is_stunned&&query.skin_id==1"
						}
					],
					"blend_transition": 0.5
				},
				"awaken": {
					"animations": [
						"awaken"
					],
					"transitions": [
						{
							"idle": "query.all_animations_finished"
						},
						{
							"move": "query.all_animations_finished"
						},
						{
							"unconscious": "query.is_stunned&&query.skin_id==0"
						}
					],
					"blend_transition": 0.4
				},
				"idle": {
					"animations": [
						"idle"
					],
					"transitions": [
						{
							"move": "q.ground_speed>0.1"
						},
						{
							"unconscious": "query.is_stunned&&query.skin_id==0"
						}
					],
					"blend_transition": 0.4
				},
				"move": {
					"animations": [
						"move"
					],
					"transitions": [
						{
							"idle": "q.ground_speed<0.1"
						},
						{
							"unconscious": "query.is_stunned&&query.skin_id==0"
						}
					],
					"blend_transition": 0.4
				}
			}
		},
		"controller.animation.verdant_sentinel.attack_controller": {
			"states": {
				"default": {
					"transitions": [
						{
							"normal_attack": "query.is_delayed_attacking==1&&query.skin_id==3"
						},
						{
							"double_attack": "query.is_delayed_attacking==1&&query.skin_id==4"
						},
						{
							"heavy_attack": "query.is_delayed_attacking==1&&query.skin_id==5"
						},
						{
							"move_attack": "query.is_delayed_attacking==1&&query.skin_id==6"
						},
						{
							"exhausted": "query.is_sheared&&query.health<=200"
						}
					],
					"blend_transition": 0.4
				},
				"normal_attack": {
					"animations": [
						"normal_attack"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					],
					"blend_transition": 0.4
				},
				"double_attack": {
					"animations": [
						"double_attack"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					],
					"blend_transition": 0.4
				},
				"heavy_attack": {
					"animations": [
						"heavy_attack"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					],
					"blend_transition": 0.4
				},
				"exhausted": {
					"animations": [
						"exhausted"
					],
					"transitions": [
						{
							"default": "!query.is_sheared"
						}
					],
					"blend_transition": 0.4
				},
				"move_attack": {
					"animations": [
						"move_attack"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					],
					"blend_transition": 0.4
				}
			}
		}
	}
}