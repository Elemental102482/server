{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.astro_jelly.blow_up_controller": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"blow_up": "query.is_sheared"
						}
					]
				},
				"blow_up": {
					"animations": [
						"blow_up"
					],
					"transitions": [
						{
							"default": "query.all_animations_finished"
						}
					],
					"on_exit": [
						"@s despawn"
					]
				}
			}
		}
	}
}