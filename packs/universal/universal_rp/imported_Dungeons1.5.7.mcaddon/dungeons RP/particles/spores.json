{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "dungeons:spores",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/forceglow_particles"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.size=0.2;variable.radius=2;"
			},
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": true
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 61,
				"max_particles": 90
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_point": {
				"offset": [
					"variable.radius*-math.sin(variable.emitter_age*360)",
					"-1*(v.emitter_age*6)",
					"variable.radius*math.cos(variable.emitter_age*360)"
				]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 1
			},
			"minecraft:particle_initial_spin": {
				"rotation_rate": 720
			},
			"minecraft:particle_initial_speed": 2,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [
					0,
					-2,
					0
				]
			},
			"minecraft:particle_appearance_billboard": {
				"size": [
					"variable.size*(1-variable.particle_age)",
					"variable.size*(1-variable.particle_age)"
				],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"uv": [
						32,
						88
					],
					"uv_size": [
						8,
						8
					]
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FFFFFFFF",
						"0.81": "#FFFF01DA",
						"1.0": "#FFFF3A75"
					}
				}
			}
		}
	}
}