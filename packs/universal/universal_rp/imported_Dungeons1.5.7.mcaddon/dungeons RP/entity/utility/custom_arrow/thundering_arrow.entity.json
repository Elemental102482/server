{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "dungeons:thundering_arrow",
			"materials": {
				"default": "arrow"
			},
			"textures": {
				"default": "textures/entity/arrows/thundering_arrow"
			},
			"geometry": {
				"default": "geometry.arrow"
			},
			"animations": {
				"move": "animation.arrow.move",
				"particles": "animation.necromancer_shot.particles"
			},
			"particle_effects": {
				"energy": "dungeons:charged_shot"
			},
			"scripts": {
				"pre_animation": [
					"variable.shake = query.shake_time - query.frame_alpha;",
					"variable.shake_power = variable.shake > 0.0 ? -Math.sin(variable.shake * 200.0) * variable.shake : 0.0;"
				],
				"animate": [
					"move",
					{
						"particles": "query.is_baby*q.variant"
					}
				]
			},
			"render_controllers": [
				"controller.render.arrow"
			]
		}
	}
}