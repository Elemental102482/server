{
	"format_version": "1.10",
	"render_controllers": {
		"controller.render.entity_guard": {
			"geometry": "geometry.default",
			"part_visibility": [
				{
					"*": true
				}
			],
			"materials": [
				{
					"*": "material.enchanted"
				}
			],
			"textures": [
				"texture.guard"
			],
			"color": {
				"r": 1,
				"g": 1,
				"b": 1,
				"a": 1
			},
			"uv_anim": {
				"offset": [
					"(math.floor(query.life_time*20)+query.frame_alpha)*-0.02",
					"(math.floor(query.life_time*20)+query.frame_alpha)*0.03"
				],
				"scale": [
					1,
					1
				]
			},
			"light_color_multiplier": 1.7,
			"ignore_lighting": false
		}
	}
}