{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.illusioner.move": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"move"
					],
					"transitions": [
						{
							"drawing": "q.has_target&&!q.is_casting"
						}
					]
				},
				"drawing": {
					"animations": [
						"move_drawing"
					],
					"transitions": [
						{
							"default": "!q.has_target||q.is_casting"
						}
					]
				}
			}
		}
	}
}