			},
			{
				"condition": "q.block_state('dungeons:rotation') >= 8 || q.block_state('minecraft:block_face') == 'south'",
				"components": {
					"minecraft:transformation": {
						"rotation": [
							0,
							180,
							0
						]
					}
				}
			},
			{
				"condition": "q.block_state('dungeons:rotation') >= 12 || q.block_state('minecraft:block_face') == 'west'",
